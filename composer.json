{
    "name": "silverstripe/selectupload",
    "description": "Enables files to be uploaded into a user-designated folder",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "upload",
        "file",
        "cwp"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Damian Mooyman",
            "email": "damian@silverstripe.com"
        }
    ],
    "require": {
        "silverstripe/framework": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "support": {
        "issues": "https://github.com/silverstripe-labs/silverstripe-selectupload/issues"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        },
        "expose": [
            "css",
            "js"
        ]
    },
    "autoload": {
        "psr-4": {
            "SilverStripe\\SelectUpload\\": "src/",
            "SilverStripe\\SelectUpload\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
